<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity LikeAlbum.
    -->
    <changeSet id="20220415122718-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="extended_user_id"
                                 baseTableName="like_album"
                                 constraintName="fk_like_album_extended_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="extended_user"/>

        <addForeignKeyConstraint baseColumnNames="album_id"
                                 baseTableName="like_album"
                                 constraintName="fk_like_album_album_id"
                                 referencedColumnNames="id"
                                 referencedTableName="album"/>

    </changeSet>
</databaseChangeLog>
